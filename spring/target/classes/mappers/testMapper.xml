<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="test.mapper.testMapper">

	<resultMap type="com.project.test.dto.UserDto" id="boardMap">
		<result property="id" column="u_id"/>
		<result property="pwd" column="u_pwd"/>
		<result property="name" column="u_name"/>
		<result property="tel" column="u_tel"/>
		<result property="birth" column="u_birth"/>
		<result property="wedding" column="u_wedding"/>
		<result property="p_birth" column="u_p_birth"/>
		<result property="email" column="u_email"/>
		<result property="address" column="u_address"/>
		<result property="start_date" column="u_start_date"/>
		<result property="code" column="u_code"/>
	</resultMap>

	<select id="selectList" resultMap="boardMap">
		SELECT *
		FROM atworks_user
	</select>
</mapper>